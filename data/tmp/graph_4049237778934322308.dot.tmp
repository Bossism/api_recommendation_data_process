  3889 [ label="72-Call
mNext.setOnClickListener(v -> {     if (mSelectedLanguagePosition >= 0 && mNext.isEnabled()) {         String languageCode = SharedPref.getStringParams(getContext(), SharedPrefsConstants.USER_SELECTED_LANGUAGE_CODE, Constants.EMPTY);         if (languageCode.equalsIgnoreCase(languageList.get(mSelectedLanguagePosition).getLanguageCode())) {             dismissAllowingStateLoss();         } else {             // Setting the user's choice of language in the shared prefs to fetch the language specific strings from the firebase and store them in shared prefs             SharedPref.setStringParams(getContext(), SharedPrefsConstants.USER_SELECTED_LANGUAGE_CODE, languageList.get(mSelectedLanguagePosition).getLanguageCode());             AnalyticsUtils.updateUserTraits();             if (mLanguageChangeListener != null) {                 mLanguageChangeListener.languageChange();                 dismissAllowingStateLoss();             }         }     } else         Toast.makeText(getContext(), R.string.please_select_a_language_to_proceed, Toast.LENGTH_SHORT).show(); })" shape="oval" line="108" fontname="helvetica" ];
